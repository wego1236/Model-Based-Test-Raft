  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s1,
    body |->
        [term |-> 4, candidate |-> s3, lastLogIdx |-> 1, lastLogTerm |-> 3],
    seq |-> 32 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s1,
    body |->
        [term |-> 5, candidate |-> s3, lastLogIdx |-> 1, lastLogTerm |-> 3],
