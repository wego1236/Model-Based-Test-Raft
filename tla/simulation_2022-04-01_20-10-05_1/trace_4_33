/\ log = ( s1 :> <<[term |-> 3, value |-> v1], [term |-> 3, value |-> v1]>> @@
  s2 :> <<>> @@
  s3 :>
      << [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v2] >> )
/\ pc = <<"Timeout: election", s2>>
/\ state = (s1 :> Follower @@ s2 :> Candidate @@ s3 :> Candidate)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 4 @@ s2 :> 6 @@ s3 :> 4)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {} @@ s2 :> {s2} @@ s3 :> {s3})
/\ scr = [ lock |->
      [ type |-> RequestVoteRequest,
        args |-> {[from |-> s2, to |-> s1], [from |-> s2, to |-> s3]} ],
  nSent |-> 56 ]
/\ votedFor = (s1 :> Nil @@ s2 :> s2 @@ s3 :> s3)


