              << [term |-> 2, value |-> v1],
                 [term |-> 2, value |-> v2],
                 [term |-> 2, value |-> v2],
                 [term |-> 2, value |-> v2] >>,
          commitIndex |-> 0,
          term |-> 2,
          entries |-> <<[term |-> 2, value |-> v1]>>,
          prevLogIndex |-> 0,
          prevLogTerm |-> 0 ],
    seq |-> 15 ],
  [ type |-> AppendEntriesRequest,
    from |-> s3,
    to |-> s2,
    body |->
        [ log |-> <<>>,
          commitIndex |-> 0,
          term |-> 2,
          entries |-> <<>>,
          prevLogIndex |-> 0,
          prevLogTerm |-> 0 ],
    seq |-> 9 ],
  [ type |-> RequestVoteRequest,
    from |-> s1,
    to |-> s3,
    body |->
        [term |-> 3, candidate |-> s1, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 12 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s1,
    body |->
        [term |-> 2, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 1 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s1,
    body |->
        [term |-> 2, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 10 ],
  [ type |-> RequestVoteResponse,
    from |-> s2,
    to |-> s1,
    body |-> [term |-> 3, voteGranted |-> TRUE],
    seq |-> 13 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<>> @@
  s2 :> <<>> @@
  s3 :>
      << [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v2] >> )
/\ pc = <<"DoLoop: AppendEntries", s3, s1, 15>>
/\ state = (s1 :> Candidate @@ s2 :> Follower @@ s3 :> Leader)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 3 @@ s2 :> 3 @@ s3 :> 2)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {} @@ s3 :> {s1, s3})
/\ scr = [ lock |-> [type |-> AppendEntriesRequest, args |-> {[from |-> s3, to |-> s2]}],
  nSent |-> 15 ]
/\ votedFor = (s1 :> s1 @@ s2 :> s1 @@ s3 :> s3)


