    body |->
        [term |-> 3, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 21 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
