    to |-> s1,
    body |->
        [term |-> 2, candidate |-> s2, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 4 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s1,
    body |->
        [term |-> 2, candidate |-> s2, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 15 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s3,
    body |->
        [term |-> 4, candidate |-> s2, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 13 ],
  [ type |-> RequestVoteResponse,
    from |-> s1,
    to |-> s2,
    body |-> [term |-> 4, voteGranted |-> FALSE],
    seq |-> 18 ],
  [ type |-> RequestVoteResponse,
    from |-> s1,
    to |-> s2,
    body |-> [term |-> 4, voteGranted |-> FALSE],
    seq |-> 20 ],
  [ type |-> RequestVoteResponse,
    from |-> s2,
    to |-> s1,
    body |-> [term |-> 3, voteGranted |-> FALSE],
    seq |-> 10 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = (s1 :> <<>> @@ s2 :> <<>> @@ s3 :> <<>>)
/\ pc = <<"Duplicate", Nil, Nil, 18>>
/\ state = (s1 :> Follower @@ s2 :> Candidate @@ s3 :> Follower)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 4 @@ s2 :> 4 @@ s3 :> 1)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2} @@ s3 :> {})
/\ scr = [nSent |-> 20, lock |-> Nil]
/\ votedFor = (s1 :> Nil @@ s2 :> s2 @@ s3 :> Nil)


