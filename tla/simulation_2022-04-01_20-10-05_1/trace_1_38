          commitIndex |-> 0,
          term |-> 2,
          entries |-> <<>>,
          prevLogIndex |-> 0,
          prevLogTerm |-> 0 ],
    seq |-> 12 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = (s1 :> <<>> @@ s2 :> <<>> @@ s3 :> <<>>)
/\ pc = <<"DoLoop: AppendEntries", s2, s3, 12>>
