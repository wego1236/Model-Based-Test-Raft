  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<[term |-> 2, value |-> v1], [term |-> 2, value |-> v2]>> @@
  s2 :> <<>> @@
  s3 :> <<>> )
/\ pc = <<"DoLoop: AppendEntries", s1, s3, 14>>
