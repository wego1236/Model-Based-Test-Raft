        [term |-> 9, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 73 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s2,
    body |->
        [term |-> 7, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 58 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s2,
