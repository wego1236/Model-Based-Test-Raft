/\ scr = [ lock |->
      [ type |-> RequestVoteRequest,
        args |-> {[from |-> s3, to |-> s1], [from |-> s3, to |-> s2]} ],
  nSent |-> 30 ]
/\ votedFor = (s1 :> s1 @@ s2 :> s2 @@ s3 :> s3)


