/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<[term |-> 2, value |-> v2]>> @@
  s2 :>
      << [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v1] >> @@
  s3 :> <<>> )
/\ pc = <<"Duplicate", Nil, Nil, 15>>
/\ state = (s1 :> Candidate @@ s2 :> Candidate @@ s3 :> Follower)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 4 @@ s2 :> 5 @@ s3 :> 3)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2} @@ s3 :> {})
/\ scr = [nSent |-> 30, lock |-> Nil]
/\ votedFor = (s1 :> s1 @@ s2 :> s2 @@ s3 :> s2)


STATE_54 == 
/\ elections = {}
/\ messages = { [ type |-> AppendEntriesResponse,
    from |-> s1,
    to |-> s2,
    body |-> [term |-> 2, success |-> TRUE, curIdx |-> 1],
    seq |-> 15 ],
  [ type |-> AppendEntriesResponse,
    from |-> s1,
    to |-> s2,
    body |-> [term |-> 2, success |-> TRUE, curIdx |-> 1],
    seq |-> 30 ],
  [ type |-> AppendEntriesResponse,
    from |-> s3,
    to |-> s2,
    body |-> [term |-> 2, success |-> TRUE, curIdx |-> 0],
    seq |-> 17 ],
  [ type |-> RequestVoteRequest,
    from |-> s1,
    to |-> s3,
    body |->
        [term |-> 4, candidate |-> s1, lastLogIdx |-> 1, lastLogTerm |-> 2],
    seq |-> 23 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s1,
    body |->
        [term |-> 4, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 24 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s1,
    body |->
        [term |-> 5, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 27 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s3,
    body |->
        [term |-> 4, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 25 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s3,
    body |->
        [term |-> 5, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 28 ],
  [ type |-> RequestVoteResponse,
    from |-> s3,
    to |-> s2,
    body |-> [term |-> 3, voteGranted |-> TRUE],
    seq |-> 29 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<[term |-> 2, value |-> v2]>> @@
  s2 :>
      << [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v1] >> @@
  s3 :> <<>> )
/\ pc = <<"Timeout: election", s3>>
/\ state = (s1 :> Candidate @@ s2 :> Candidate @@ s3 :> Candidate)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 4 @@ s2 :> 5 @@ s3 :> 4)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
