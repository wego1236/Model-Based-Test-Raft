    to |-> s1,
    body |->
        [term |-> 5, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 27 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s3,
    body |->
        [term |-> 4, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 25 ],
  [ type |-> RequestVoteRequest,
    from |-> s2,
    to |-> s3,
    body |->
        [term |-> 5, candidate |-> s2, lastLogIdx |-> 3, lastLogTerm |-> 2],
    seq |-> 28 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s1,
    body |->
        [term |-> 4, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 31 ],
  [ type |-> RequestVoteRequest,
    from |-> s3,
    to |-> s2,
    body |->
        [term |-> 4, candidate |-> s3, lastLogIdx |-> 0, lastLogTerm |-> 0],
    seq |-> 32 ],
  [ type |-> RequestVoteResponse,
    from |-> s3,
    to |-> s2,
    body |-> [term |-> 3, voteGranted |-> TRUE],
    seq |-> 29 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<[term |-> 2, value |-> v2]>> @@
  s2 :>
      << [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v1] >> @@
  s3 :> <<>> )
/\ pc = <<"DoLoop: RequestVote", s1, s3, 34>>
/\ state = (s1 :> Candidate @@ s2 :> Candidate @@ s3 :> Candidate)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 5 @@ s2 :> 5 @@ s3 :> 4)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2} @@ s3 :> {s3})
/\ scr = [lock |-> Nil, nSent |-> 34]
/\ votedFor = (s1 :> s1 @@ s2 :> s2 @@ s3 :> s3)


