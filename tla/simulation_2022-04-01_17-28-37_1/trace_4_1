/\ scr = [ lock |-> [type |-> RequestVoteRequest, args |-> {[from |-> s3, to |-> s2]}],
  nSent |-> 20 ]
/\ votedFor = (s1 :> s3 @@ s2 :> s2 @@ s3 :> s3)


