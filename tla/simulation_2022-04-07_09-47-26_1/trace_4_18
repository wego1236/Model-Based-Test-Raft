  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<>> @@
  s2 :> <<>> @@
  s3 :>
      << [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v3],
         [term |-> 2, value |-> v5] >> )
/\ pc = <<"DoLoop: RequestVote", s3, s1, 47>>
/\ state = (s1 :> Candidate @@ s2 :> Follower @@ s3 :> Candidate)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 9 @@ s2 :> 8 @@ s3 :> 10)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2} @@ s3 :> {s3})
/\ scr = [ lock |-> [type |-> RequestVoteRequest, args |-> {[from |-> s3, to |-> s2]}],
  nSent |-> 47 ]
/\ votedFor = (s1 :> s1 @@ s2 :> Nil @@ s3 :> s3)


