  [ type |-> RequestVoteResponse,
    from |-> s2,
    to |-> s3,
    body |-> [term |-> 8, voteGranted |-> FALSE],
    seq |-> 46 ],
  [ type |-> RequestVoteResponse,
    from |-> s2,
    to |-> s3,
    body |-> [term |-> 12, voteGranted |-> FALSE],
    seq |-> 59 ],
  [ type |-> RequestVoteResponse,
    from |-> s3,
    to |-> s1,
    body |-> [term |-> 4, voteGranted |-> FALSE],
    seq |-> 19 ] }
/\ matchIndex = ( s1 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s2 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) @@
  s3 :> (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0) )
/\ log = ( s1 :> <<>> @@
  s2 :> <<>> @@
  s3 :>
      << [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v2],
         [term |-> 2, value |-> v1],
         [term |-> 2, value |-> v3],
         [term |-> 2, value |-> v5] >> )
/\ pc = <<"DoLoop: RequestVote", s3, s2, 61>>
/\ state = (s1 :> Candidate @@ s2 :> Follower @@ s3 :> Candidate)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 11 @@ s2 :> 12 @@ s3 :> 13)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2} @@ s3 :> {s3})
/\ scr = [lock |-> Nil, nSent |-> 61]
/\ votedFor = (s1 :> s1 @@ s2 :> Nil @@ s3 :> s3)


