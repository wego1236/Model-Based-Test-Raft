  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
/\ votesGranted = (s1 :> {s1} @@ s2 :> {s2, s3} @@ s3 :> {})
/\ scr = [lock |-> Nil, nSent |-> 9]
/\ votedFor = (s1 :> s1 @@ s2 :> s2 @@ s3 :> s2)


