  s3 :> <<[term |-> 2, value |-> v5]>> )
/\ pc = <<"HandleRequestVoteRequest: demote and not granted ", s2, s1, 23>>
/\ state = (s1 :> Candidate @@ s2 :> Follower @@ s3 :> Follower)
/\ commitIndex = (s1 :> 0 @@ s2 :> 0 @@ s3 :> 0)
/\ currentTerm = (s1 :> 6 @@ s2 :> 6 @@ s3 :> 4)
/\ nextIndex = ( s1 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s2 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) @@
  s3 :> (s1 :> 1 @@ s2 :> 1 @@ s3 :> 1) )
